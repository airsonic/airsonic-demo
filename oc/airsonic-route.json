{
  "apiVersion": "v1",
  "kind": "Route",
  "metadata": {
    "labels": {
      "app": "airsonic"
    },
    "name": "airsonic",
    "namespace": "airsonic-demo"
  },
  "spec": {
    "path": "/",
    "port": {
      "path": "/",
      "targetPort": "8080-tcp"
    },
    "to": {
      "kind": "Service",
      "name": "airsonic",
      "weight": 100
    },
    "wildcardPolicy": "None"
  }
}